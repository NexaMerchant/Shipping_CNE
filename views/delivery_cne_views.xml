<odoo>
    <data>
        <record id="view_delivery_cne_tree" model="ir.ui.view">
            <field name="name">delivery_cne.tree</field>
            <field name="model">delivery_cne.delivery_cne</field>
            <field name="arch" type="xml">
                <list string="CNE Deliveries">
                    <field name="name"/>
                    <field name="shipping_number"/>
                    <field name="shipping_status"/>
                </list>
            </field>
        </record>

        <record id="view_delivery_cne_form" model="ir.ui.view">
            <field name="name">delivery_cne.form</field>
            <field name="model">delivery_cne.delivery_cne</field>
            <field name="arch" type="xml">
                <form string="CNE Delivery" class="o_sale_order">
                    <header>
                        <button name="action_get_cne_delivery_number"
                                type="object"
                                string="Get CNE Delivery Number"
                                class="oe_highlight"/>
                    </header>
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="carrier_id"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>